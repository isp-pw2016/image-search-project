@using Isp.Core.Configs
@using Isp.Core.Entities
@using Isp.Core.Extensions
@using Isp.Web.Extensions
@{
    <script type="text/javascript">
        window.constants = {
            'paths': {
                'getImages': '@Url.AbsoluteAction("GetImages", "Home")'
            },
            'enums': {
                'google': '@ImageFetchHandler.Google.ToLong()',
                'bing': '@ImageFetchHandler.Bing.ToLong()',
                'instagram': '@ImageFetchHandler.Instagram.ToLong()',
                'flickr': '@ImageFetchHandler.Flickr.ToLong()',
                'shutterstock': '@ImageFetchHandler.Shutterstock.ToLong()'
            },
            'configs': {
                'googleApiUrl': '@AppSetting.GoogleApiUrl',
                'googleApiKey': '@AppSetting.GoogleApiKey',
                'googleEngineId': '@AppSetting.GoogleEngineId',
                'bingApiUrl': '@AppSetting.BingApiUrl',
                'bingApiKey': '@AppSetting.BingApiKey',
                'instagramApiUrl': '@AppSetting.InstagramApiUrl',
                'instagramAccessToken': '@AppSetting.InstagramAccessToken',
                'flickrApiUrl': '@AppSetting.FlickrApiUrl',
                'flickrApiKey': '@AppSetting.FlickrApiKey',
                'flickrPhotoUrl': '@AppSetting.FlickrPhotoUrl',
                'shutterstockApiUrl': '@AppSetting.ShutterstockApiUrl',
                'shutterstockClientId': '@AppSetting.ShutterstockClientId',
                'shutterstockClientSecret': '@AppSetting.ShutterstockClientSecret',
                'shutterstockCredentials': '@AppSetting.ShutterstockCredentials'
            }
        };
